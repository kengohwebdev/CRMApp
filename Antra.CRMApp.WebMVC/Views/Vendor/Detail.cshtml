@using Antra.CRMApp.Core.Model
@model List<VendorResponseModel>



<h1>Category details</h1>
<table class="table">
    <thead>
        <tr>
           
            <td>Name</td>
            <td>Email</td>
            <td>Phone</td>
            <td>City</td>
            <td>Country</td>
            <td>Region</td>
            <td>Active Status</td>
        
        </tr>
    </thead>
    <tbody>
        @{
            foreach(var m in Model)
            {
               <tr>
               <td>@m.Name</td>
               <td>@m.EmailId</td>
               <td>@m.Mobile</td>
               <td>@m.City</td>
               <td>@m.Country</td>
               <td>@m.RegionId</td>
               <td>@m.IsActive</td>
             
               </tr> 

            }
        }
    </tbody>
</table>



<a asp-action="Index" asp-controller="Vendor"  class="btn btn-primary">Back</a>
