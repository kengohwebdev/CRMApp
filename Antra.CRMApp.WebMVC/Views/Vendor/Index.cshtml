@model IEnumerable<VendorModel>
<a asp-action="Create" asp-controller="Vendor" class="btn bg-success" >Add Vendor</a>
<table class="table table-striped">
  <thead>
      <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Email Id</th>
      </tr>
       <tr>
          <th>City</th>
          <th>Country</th>
          <th>Mobile</th>
      </tr>
      <tr><th>Active</th></tr>
  </thead>
  <tbody>
      @foreach(var v in Model)
        {
            <tr>
                <td>@v.Id</td>
                <td>@v.Name</td>
                <td>@v.EmailId</td>
            </tr>
              <tr>
                <td>@v.City</td>
                <td>@v.Country</td>
                <td>@v.Mobile</td>
            </tr>
            <tr><th>Active</th></tr>
        }
    </tbody>
</table>

 