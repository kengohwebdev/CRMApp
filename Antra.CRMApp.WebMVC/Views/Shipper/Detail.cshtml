@using Antra.CRMApp.Core.Model
@model List<ShipperModel>



<h1>Product details</h1>
<table class="table">
    <thead>
        <tr>
            
            <td>Name</td>
            <td>Phone</td>
       
        </tr>
    </thead>
    <tbody>
        @{
            foreach(var m in Model)
            {
               <tr>
               <td>@m.Name</td>
               <td>@m.Phone</td>
               </tr> 

            }
        }
    </tbody>
</table>


<a asp-action="Index" asp-controller="Shipper"  class="btn btn-primary">Back</a>
